<view class="container">
  <view class="userinfo">
    
    <view class="item-title">
      <image class="logoimg" src="/pages/images/ils-nuli@2x.png" mode="cover"></image>
    </view>
    <view class="item-gongxi">很遗憾！</view>
  
    <view class="item-huode">{{failmsg}}</view>
    
    <button wx:if="{{ fujiati != '1' && count != 10}}" class="userinfobtn" bindtap="gonext"> 再试一次还有机会 </button>
    <button wx:if="{{ fujiati != '1' && count == 10}}" class="userinfobtn" bindtap="gonext"> 下个任务无机会了 </button>
    <button wx:if="{{count == '2' && fujiati == '1'}}" class="userinfobtn" bindtap="gonext3"> 再试一次加分任务 </button>
    <!-- 加分任务时候显示 -->
    <button wx:if="{{count == '2' && fujiati == '1'}}" class="userinfobtn" bindtap="gonext2"> 跳过加分任务 </button>
  </view>
  
</view>
